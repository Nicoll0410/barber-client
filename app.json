{
  "expo": {
    "name": "barberia-client",
    "slug": "barberia-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./public/apple-touch-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/apple-touch-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.barberiaclient"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    }
  }
}